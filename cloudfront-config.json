{
  "CallerReference": "nandhakumar-voice-assistant-ai-2024",
  "Comment": "Nandhakumar's Voice Assistant AI CloudFront Distribution",
  "DefaultCacheBehavior": {
    "TargetOriginId": "S3-nandhakumar-voice-assistant-ai",
    "ViewerProtocolPolicy": "redirect-to-https",
    "MinTTL": 0,
    "ForwardedValues": {
      "QueryString": false,
      "Cookies": {
        "Forward": "none"
      }
    },
    "TrustedSigners": {
      "Enabled": false,
      "Quantity": 0
    }
  },
  "Origins": {
    "Quantity": 1,
    "Items": [
      {
        "Id": "S3-nandhakumar-voice-assistant-ai",
        "DomainName": "nandhakumar-voice-assistant-ai.s3-website-us-east-1.amazonaws.com",
        "CustomOriginConfig": {
          "HTTPPort": 80,
          "HTTPSPort": 443,
          "OriginProtocolPolicy": "http-only"
        }
      }
    ]
  },
  "Enabled": true,
  "DefaultRootObject": "public/index.html"
}
